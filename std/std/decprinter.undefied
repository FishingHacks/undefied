include "std/io"

memory pd_tmp_store 32 end

fn printdec int in
dup 1 < if drop return end
31 swap
while
    10 /% '0' +
    rot swap
    dup swap rot pd_tmp_store cast(int) + cast(ptr) !
    1 -
    swap
    dup 1 <
    if drop 0 end
end  
drop dup 32 swap - pd_tmp_store cast(int) rot rot + cast(ptr) puts
end
